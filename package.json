{
  "name": "@kitbag/router",
  "private": false,
  "version": "0.0.3",
  "bugs": {
    "url": "https://github.com/kitbagjs/router/issues"
  },
  "homepage": "https://github.com/kitbagjs/router#readme",
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "test:types": "vitest typecheck",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "types": "tsc --noEmit"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/kitbag-router.umd.cjs",
  "module": "./dist/kitbag-router.js",
  "types": "./dist/kitbag-router.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kitbag-router.js",
      "require": "./dist/kitbag-router.umd.cjs"
    }
  },
  "devDependencies": {
    "@prefecthq/eslint-config": "^1.0.31",
    "eslint": "^8.54.0",
    "happy-dom": "^12.10.3",
    "typescript": "^5.0.2",
    "vite": "^4.5.1",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "vue": "^3.3.8"
  }
}
